shade to industria: Costs 1LY FTL
action/ftl/?system_id=602
action/ftl/?system_id=600
action/ftl/?system_id=2314 //industria
(async () => {
   res = await apiRequest("POST", "action/ftl/?system_id=602", true);
     console.log(res.success);
   res = await apiRequest("POST", "action/ftl/?system_id=600", true);
     console.log(res.success);
   res = await apiRequest("POST", "action/ftl/?system_id=2314", true);
     console.log(res.success);
})();

    


