(async () => {
   res = await apiRequest("POST", "action/ftl/?system_id=600", true); //falling rocks
     console.log(res.success);
   res = await apiRequest("POST", "action/ftl/?system_id=602", true); //balo martim
     console.log(res.success);
   res = await apiRequest("POST", "action/ftl/?system_id=3940", true); //shade
     console.log(res.success);
   res = await apiRequest("POST", "action/ftl/?system_id=307", true); //tesh
     console.log(res.success);
   res = await apiRequest("POST", "action/ftl/?system_id=309", true); //coroscar
     console.log(res.success);     
   res = await apiRequest("POST", "action/ftl/?system_id=3632", true); //kill space
     console.log(res.success);  
   res = await apiRequest("POST", "action/ftl/?system_id=2388", true); //marketplace
     console.log(res.success);
   res = await apiRequest("POST", "action/ftl/?system_id=3227", true); //farreach
     console.log(res.success);
})();

